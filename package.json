{
  "$schema": "https://json.schemastore.org/package",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "git": "git add . && git commit -m \"$(date '+%-m').$(date '+%-d').$(date '+%-H')\"",
    "upd": "pnpm ncu -u",
    "lint": "pnpm prettier --write \"src/**/*.{ts,tsx,scss}\" --log-level warn && pnpm eslint --fix",
    "dev": "pnpm vite",
    "build": "pnpm ts-node-esm build.ts",
    "build:p": "pnpm ts-node-esm build.ts -p",
    "preview": "pnpm vite preview --open"
  },
  "dependencies": {
    "@fvilers/disable-react-devtools": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "clsx": "^2.0.0",
    "cssnano": "^6.0.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-check-updates": "^16.10.16",
    "postcss": "^8.4.26",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "simple-zustand-devtools": "^1.1.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "terser": "^5.19.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.4"
  }
}
